{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "develop": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watch",
    "db:reset": "echo placeholder until we have reset db command",
    "build:test": "yarn db:reset && export NODE_ENV=\"test\" && next build",
    "start:test": "export NODE_ENV=\"test\" && next start",
    "cypress:firefox": "env-cmd -f .env.test.local cypress open --e2e --browser firefox",
    "cypress:edge": "env-cmd -f .env.test.local cypress open --e2e --browser edge",
    "cypress:chrome": "env-cmd -f .env.test.local cypress open --e2e --browser chrome",
    "cypress:open": "env-cmd -f .env.test.local cypress open --e2e",
    "cypress:start": "concurrently \"start-server-and-test start:test 3001 cypress:open\" \"yarn --cwd ../server serve\"",
    "cypress:build": "yarn build:test && yarn cypress:start"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "@svgr/webpack": "^6.5.1",
    "@types/node": "18.11.10",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "async-mutex": "^0.4.0",
    "axios": "^1.2.0",
    "bcryptjs": "^2.4.3",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.5",
    "jsonwebtoken": "^8.5.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "matter-js": "^0.18.0",
    "next": "13.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "replicator": "^1.0.5",
    "socket.io-client": "^4.5.4",
    "typescript": "4.9.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/imap": "^0.8.35",
    "@types/matter-js": "^0.18.2",
    "@types/nodemailer": "^6.4.7",
    "@types/uuid": "^9.0.0",
    "concurrently": "^7.6.0",
    "cypress": "^11.2.0",
    "cypress-recurse": "^1.26.0",
    "env-cmd": "^10.1.0",
    "imap": "^0.8.19",
    "imap-simple": "^5.1.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "mailparser": "^3.6.3",
    "nodemailer": "^6.8.0",
    "sass": "^1.57.1",
    "start-server-and-test": "^1.14.0"
  }
}
