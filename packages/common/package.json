{
  "name": "@lucella/common",
  "main": "dist/index",
  "version": "1.0.0",
  "license": "MIT",
  "watch": {
    "compile-proto": {
      "patterns": [
        "./src/proto/*.proto"
      ],
      "extensions": "proto",
      "quiet": false
    },
    "compile-proto-windows": {
      "patterns": [
        "./src/proto/*.proto"
      ],
      "extensions": "proto",
      "quiet": false
    }
  },
  "scripts": {
    "compile-proto": "./src/proto/compile-proto.sh",
    "compile-proto-windows": "bash ./src/proto/compile-proto-windows.sh",
    "watch-and-compile-proto": "yarn run npm-watch compile-proto",
    "watch-and-compile-proto-windows": "yarn run npm-watch compile-proto-windows"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0",
    "matter-js": "^0.18.0",
    "replicator": "^1.0.5",
    "uuid": "^9.0.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/matter-js": "^0.18.2",
    "@types/uuid": "^9.0.0",
    "npm-watch": "^0.11.0",
    "ts-protoc-gen": "^0.15.0"
  }
}
