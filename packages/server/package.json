{
  "name": "lucella",
  "version": "1.0.0",
  "description": "Lucella: Latin - small gains or profits.",
  "main": "server.js",
  "scripts": {
    "build": "rimraf dist && yarn compile-proto && tsc --build",
    "serve": "tsc-watch -b --onSuccess \"node dist/index.js\"",
    "server": "nodemon server",
    "servers": "nodemon servers",
    "client": "yarn --cwd ../client develop",
    "dev": "concurrently \"yarn serve\" \"yarn client\" \"yarn watch-and-compile-proto\"",
    "dev-windows": "concurrently \"yarn serve\" \"yarn client\" \"yarn watch-and-compile-proto-windows\"",
    "migrate": "node-pg-migrate",
    "test": "jest --no-cache",
    "compile-proto": "yarn --cwd ../common compile-proto",
    "compile-proto-windows": "yarn --cwd ../common compile-proto-windows",
    "watch-and-compile-proto": "yarn --cwd ../common watch-and-compile-proto",
    "watch-and-compile-proto-windows": "yarn --cwd ../common watch-and-compile-proto-windows"
  },
  "author": "Mike Silverman",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.8",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-validator": "^6.4.0",
    "helmet": "^3.21.2",
    "jsonwebtoken": "9.0.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.values": "^4.3.0",
    "matter-js": "^0.18.0",
    "nodemailer": "^6.4.2",
    "pg": "^8.8.0",
    "pg-format": "^1.0.4",
    "redis": "^4.3.1",
    "replicator": "^1.0.5",
    "socket.io": "^4.5.2",
    "socketio-jwt-auth": "^0.1.0",
    "ts-node-dev": "^2.0.0",
    "uuid": "^7.0.2",
    "zod": "^3.19.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "^3.3.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.2.2",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/lodash.isequal": "^4.5.6",
    "@types/matter-js": "^0.18.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.23",
    "@types/nodemailer": "^6.4.6",
    "@types/pg": "^8.6.5",
    "@types/pg-format": "^1.0.2",
    "@types/supertest": "^2.0.12",
    "concurrently": "^5.1.0",
    "dedent": "^0.7.0",
    "jest": "^29.3.1",
    "jest-express": "^1.12.0",
    "node-pg-migrate": "^6.2.2",
    "nodemon": "^2.0.2",
    "object-sizeof": "^1.6.1",
    "socket.io-client": "^4.5.4",
    "supertest": "^6.3.3",
    "toxiproxy-node-client": "^2.0.6",
    "ts-jest": "^29.0.3",
    "tsc-watch": "^5.0.3"
  }
}
