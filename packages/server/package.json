    "@types/bcrypt": "5.0.0",
    "@types/bcryptjs": "2.4.2",
    "@types/config": "3.3.0",
    "@types/cookie-parser": "1.4.7",
    "@types/express": "4.17.14",
    "@types/jest": "29.2.2",
    "@types/jsonwebtoken": "9.0.0",
    "@types/lodash.clonedeep": "4.5.7",
    "@types/lodash.isequal": "4.5.6",
    "@types/matter-js": "0.18.2",
    "@types/morgan": "1.9.3",
    "@types/node": "20.2.5",
    "@types/nodemailer": "6.4.6",
