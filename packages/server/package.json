{
  "name": "lucella",
  "version": "1.0.0",
  "description": "Lucella: Latin - small gains or profits.",
  "main": "server.js",
  "scripts": {
    "build": "rimraf dist && yarn compile-proto && tsc --build",
    "serve": "tsc-watch -b --onSuccess \"node dist/index.js\"",
    "server": "nodemon server",
    "servers": "nodemon servers",
    "client": "yarn --cwd ../client develop",
    "dev": "concurrently \"yarn serve\" \"yarn client\" \"yarn watch-and-compile-proto\"",
    "dev-windows": "concurrently \"yarn serve\" \"yarn client\" \"yarn watch-and-compile-proto-windows\"",
    "migrate": "node-pg-migrate",
    "test": "jest --no-cache",
    "compile-proto": "yarn --cwd ../common compile-proto",
    "compile-proto-windows": "yarn --cwd ../common compile-proto-windows",
    "watch-and-compile-proto": "yarn --cwd ../common watch-and-compile-proto",
    "watch-and-compile-proto-windows": "yarn --cwd ../common watch-and-compile-proto-windows",
    "dev-with-proto": "concurrently \"yarn serve\" \"yarn watch-and-compile-proto\" \"yarn --cwd ../common tsc-watch\""
  },
  "author": "Mike Silverman",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "7.7.0",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@typescript-eslint/parser": "5.47.0",
    "bcryptjs": "2.4.3",
    "config": "3.3.8",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "envalid": "^8.0.0",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.31.11",
    "express": "4.18.1",
    "express-validator": "7.0.1",
    "helmet": "7.0.0",
    "jsonwebtoken": "9.0.0",
    "lodash.clonedeep": "4.5.0",
    "lodash.isequal": "4.5.0",
    "lodash.values": "4.3.0",
    "matter-js": "0.18.0",
    "morgan": "1.10.0",
    "nodemailer": "6.4.2",
    "pg": "8.8.0",
    "pg-format": "1.0.4",
    "prettier": "2.8.1",
    "pythonia": "1.0.1",
    "redis": "4.6.4",
    "socket.io": "4.6.1",
    "socketio-jwt-auth": "0.2.1",
    "ts-node-dev": "2.0.0",
    "uuid": "9.0.0",
    "zod": "3.19.1"
  },
