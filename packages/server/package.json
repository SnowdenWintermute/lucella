{
  "name": "lucella",
  "version": "1.0.0",
  "description": "Lucella: Latin - small gains or profits.",
  "main": "server.js",
  "scripts": {
    "build": "rimraf dist && tsc --build",
    "serve": "tsc-watch -b --onSuccess \"node dist/servers.js\"",
    "start": "node servers",
    "server": "nodemon server",
    "servers": "nodemon servers",
    "client": "yarn --cwd ../lucella-client develop",
    "dev": "concurrently \"yarn serve\" \"yarn client\""
  },
  "author": "Mike Silverman",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.2.5",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-validator": "^6.4.0",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.values": "^4.3.0",
    "mongoose": "^6.6.1",
    "nodemailer": "^6.4.2",
    "socket.io": "^4.5.2",
    "socketio-jwt-auth": "^0.1.0",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/nodemailer": "^6.4.6",
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2",
    "object-sizeof": "^1.6.1",
    "tsc-watch": "^5.0.3"
  }
}
